{{#> components/page/page align="center" }}
    <div class="back-button">
        {{#> components/icon-button/icon-button view="outline" onclick="window.location.href = '/src/pages/chats/chats.html'" }}
            {{> components/icons/arrow-left fill="#2B4526" }}
        {{/components/icon-button/icon-button}}
    </div>
    {{#> components/avatar/avatar size="130px"  }}
        {{> components/icons/upload size="24" fill="#FFFFFF" class="cursor-pointer" }}
        {{> components/icons/trash size="24" fill="#FFFFFF" class="cursor-pointer" }}
    {{/components/avatar/avatar}}
    {{#> components/form/form }}
        {{> components/input/input type="email" id="email" name="email" label="Почта" value="ioaksenenko@gmail.com" readonly="true" icon="edit" }}
        {{> components/input/input type="text" id="login" name="login" label="Логин" value="ioaksenenko" readonly="readonly" icon="edit" }}
        {{> components/input/input type="text" id="first_name" name="first_name" label="Имя" value="Иван" readonly="readonly" icon="edit" }}
        {{> components/input/input type="text" id="second_name" name="second_name" label="Фамилия" value="Аксененко" readonly="readonly" icon="edit" }}
        {{> components/input/input type="text" id="phone" name="phone" label="Телефон" value="+7 (961) 218-61-43" readonly="readonly" icon="edit" }}
        {{> components/input/input type="password" id="password" name="password" label="Пароль" value="qwerty" readonly="readonly" icon="edit" }}
    {{/components/form/form}}
    {{#> components/form/form action="/src/pages/login/login.html" }}
        {{#> components/button/button }}
            {{#> components/typography/typography type="submit" variant="bodyL" color="white" }}
                Выход
            {{/components/typography/typography}}
        {{/components/button/button}}
    {{/components/form/form}}
{{/components/page/page}}